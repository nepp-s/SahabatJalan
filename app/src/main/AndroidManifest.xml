<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCapstone"
        tools:targetApi="31">
        <activity
            android:name=".ui.DetailDestinationActivity"
            android:exported="false" />
        <activity
            android:name=".ui.Menu"
            android:exported="false"
            android:label="@string/title_activity_menu" />
        <activity
            android:name=".onboarding.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".onboarding.FourthOnBoardingActivity"
            android:exported="false" />
        <activity
            android:name=".onboarding.ThirdOnBoardingActivity"
            android:exported="false" />
        <activity
            android:name=".onboarding.SecondOnBoardingActivity"
            android:exported="false" />
        <activity
            android:name=".onboarding.FirstOnBoardingActivity"
            android:exported="false" />
        <activity
            android:name=".onboarding.IntroActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>